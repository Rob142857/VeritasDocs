{
  "name": "veritas-documents",
  "version": "1.0.0",
  "description": "NFT-based legal document storage with post-quantum cryptography",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "npm run build:frontend && npm run upload-docs && npm run upload-frontend && wrangler deploy",
    "build": "tsc",
    "build:frontend": "node esbuild.frontend.js",
    "upload-frontend": "node scripts/upload-frontend.js",
    "upload-docs": "node scripts/upload-docs.js",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@maatara/core-pqc": "^0.2.3",
    "@maatara/core-pqc-wasm": "^0.2.0",
    "viem": "^2.12.0",
    "hono": "^4.0.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.25.10",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.41.0"
  },
  "keywords": [
    "nft",
    "legal-documents",
    "post-quantum-cryptography",
    "cloudflare-workers",
    "blockchain"
  ],
  "author": "Veritas Documents",
  "license": "MIT"
}
